<h2> Lista de Centrales de Costos</h2>
<p> <%= link_to "Crear nueva Central de Costos", new_centrals_path %> </p>
<ul>
   <% @centrals.each do |t| %>
   <li>
      <h3> <%= t.nombre %> </h3>
      <p>
         <%= link_to "Ver", t %>
         |
         <%= link_to "Editar", edit_centrals_path(t) %>
         |
         <%= link_to "Eliminar", t, :confirm => "Seguro desea eliminar esta Central?", :method => :delete %>
      </p>
   </li>
   <% end %>
</ul>
